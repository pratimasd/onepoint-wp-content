{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "onepoint/header",
  "version": "0.1.0",
  "title": "Onepoint Header",
  "category": "theme",
  "icon": "layout",
  "description": "Site header with logo, navigation menu, and icons.",
  "keywords": ["header", "nav", "onepoint"],
  "textdomain": "onepoint-custom-blocks",
  "attributes": {
    "siteName": {
      "type": "string",
      "default": ""
    },
    "menuLocation": {
      "type": "string",
      "default": "primary"
    },
    "iconUrl": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "align": ["full"],
    "spacing": { "margin": true }
  },
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/style-index.css",
  "style": "file:./build/style-index.css"
}
